    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
